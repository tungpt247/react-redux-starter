{
  "name": "react-redux-starter",
  "version": "1.0.14",
  "description": "Starter kit for React and Redux",
  "scripts": {
    "prestart": "babel-node tools/prestart.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'src/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "release": "sh ./scripts/release",
    "publish": "sh ./scripts/publish"
  },
  "author": "Tung Phan",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "jquery": "^3.0.0",
    "method-override": "^2.3.6",
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cheerio": "^0.20.0",
    "clean-webpack-plugin": "^0.1.9",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-recommended-bump": "^0.2.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-minifier": "^2.1.6",
    "html-webpack-plugin": "^2.21.0",
    "html-webpack-template": "^5.0.0",
    "jquery": "^3.0.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.1.2",
    "open": "0.0.5",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tungpt247/react-redux-starter"
  }
}
